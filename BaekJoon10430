import java.util.Scanner;

public class BaekJoon10430 {
	public static void main(String args[])
	{
		Scanner input = new Scanner (System.in);
		int A;
		int B;
		int C;
		
		A = input.nextInt();
		B = input.nextInt();
		C = input.nextInt();
		
		System.out.println((A+B)%C);
		System.out.println(((A%C) + (B%C))%C);
		System.out.println((A*B)%C);
		System.out.println(((A%C) * (B%C))%C);
	}
}
