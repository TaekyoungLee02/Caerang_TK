import java.util.Scanner;

class Main {
	public static void main(String args[])
	{
		Scanner input = new Scanner (System.in);
		int A;
		int B;
		int C;
		int D;
		
		A = input.nextInt();
		B = input.nextInt();
		C = input.nextInt();
		D = input.nextInt();
		
		C += D;
		
		if (C > 59)
		{
			B += C / 60;
			C = C % 60;
			if (B > 59)
			{
				A += B / 60;
				B = B % 60;
				if (A > 23)
				{
					A = A % 24;
				}
			}
		}
		
		System.out.println(A + " " + B + " " + C);
	}
}
