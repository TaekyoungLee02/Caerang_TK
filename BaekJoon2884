import java.util.Scanner;

public class BaekJoon2884 {
	public static void main(String args[])
	{
		Scanner input = new Scanner (System.in);
		int H;
		int M;
		
		H = input.nextInt();
		M = input.nextInt();
		
		if (M < 45)
		{
			M += 15;
			if (H == 0)
			{
				H = 23;
			}
			else
			{
				H -= 1;
			}
		}
		else
		{
			M -= 45;
		}	
		
		System.out.println(H + " " + M);
	}
}
